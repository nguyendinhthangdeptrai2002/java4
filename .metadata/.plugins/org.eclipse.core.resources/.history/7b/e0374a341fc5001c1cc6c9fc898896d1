package code_thieu_nhi;

public class mayTinhDonGian {

	
	public static void main(String[] args) {
	public static void tinhtoan(){
	    Scanner nhap = new Scanner(System.in);
	     int a= nhap.nextInt;
	    String pheptoan = nhap.nextLine;
	    int b= nhap.nextInt;
	    double tinh;
	    if(pheptoan==+){
	        tinh=a+b;
	       return tinh;
	    }
	     else if(pheptoan==-){
	        tinh=a-b;
	       return tinh;
	    }
	     else if(pheptoan==+){
	        tinh=a*b;
	       return tinh;
	    }
	     else if(pheptoan==/){
	        tinh=a/b;
	       return tinh;
	    }
	    System.out.println(tinh);
	}
}
}
